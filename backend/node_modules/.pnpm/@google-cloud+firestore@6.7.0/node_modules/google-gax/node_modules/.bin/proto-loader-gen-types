#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@grpc+proto-loader@0.7.8/node_modules/@grpc/proto-loader/build/bin/proto-loader-gen-types.js" "$@"
else
  exec node  "$basedir/../../../../../@grpc+proto-loader@0.7.8/node_modules/@grpc/proto-loader/build/bin/proto-loader-gen-types.js" "$@"
fi
