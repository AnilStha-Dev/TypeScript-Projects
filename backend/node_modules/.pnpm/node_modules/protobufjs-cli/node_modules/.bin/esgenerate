#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../../../../../escodegen@1.14.3/node_modules/escodegen/bin/esgenerate.js" "$@"
fi
