#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules/protobufjs-cli/bin/pbjs" "$@"
else
  exec node  "$basedir/../../../../../protobufjs-cli@1.1.1_protobufjs@7.2.4/node_modules/protobufjs-cli/bin/pbjs" "$@"
fi
